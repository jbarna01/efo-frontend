{
  "name": "efo-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run generate:api",
    "start": "ng serve --port 8081",
    "prebuild": "npm run generate:api",
    "build": "ng build",
    "generate:api": "node openapi-clean.js && openapi-generator-cli generate -g typescript-angular -i http://localhost:8080/v3/api-docs -o ./build/openapi/efo --skip-validate-spec",
    "generate:api_fromfile": "node openapi-clean.js && openapi-generator-cli generate -g typescript-angular -i efo-openapi.json -o ./build/openapi/efo --skip-validate-spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "node openapi-clean.js",
    "initialize": "npm install",
    "generate-sources": "npm run generate:api_fromfile",
    "prepare-package": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "12.0.4",
    "@angular/compiler": "12.0.4",
    "@angular/core": "12.0.4",
    "@angular/forms": "12.0.4",
    "@angular/material": "^12.0.4",
    "@angular/platform-browser": "12.0.4",
    "@angular/platform-browser-dynamic": "12.0.4",
    "@angular/router": "12.0.4",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "angular-moment": "^1.3.0",
    "bootstrap": "^5.1.3",
    "moment": "^2.29.1",
    "ngx-mask": "^13.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "12.0.4",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.2.4"
  }
}
